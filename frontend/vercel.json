{
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 60
    },
    "app/api/ai/**/*.ts": {
      "maxDuration": 60
    },
    "app/api/proxy/**/*.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}